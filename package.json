{
  "name": "uxie",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "prepare": "node -e \"if(process.env.NODE_ENV !== 'production'){require('child_process').execSync('pnpm exec husky', {stdio: 'inherit'})}\""
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.7.2",
    "supabase": "^2.67.1",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
